  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node --trace-warnings ./src/server/server.babel.js",
    "start:tunnel": "export $(grep ^ULTRAHOOK_API_KEY .env | tr -d '\"') && ultrahook -k $ULTRAHOOK_API_KEY dev 3000",
    "quickstart": "npm run db:migrate && npm run build && npm run start",
    "dev": "if [ ! -f \"dll/vendors.json\" ] ; then npm run build:dll ; fi ; NODE_ENV=development node --trace-warnings ./src/server/server.babel.js",
    "bs": "npm run build && npm start",
    "build": "rimraf build && npm run build:deps && concurrently \"npm run build:client\" \"npm run build:server\"",
    "build:client": "NODE_ENV=production webpack --config ./webpack/prod.babel.js",
    "build:client-deploy": "NODE_ENV=production WEBPACK_DEPLOY=true WEBPACK_MIN=true webpack --config ./webpack/prod.babel.js",
    "build:client-min": "NODE_ENV=production WEBPACK_MIN=true webpack --config ./webpack/prod.babel.js",
    "build:deploy": "rimraf build && npm run build:deps && npm run build:server && npm run build:client-deploy",
    "build:deps": "concurrently \"npm run build:theme\" \"npm run build:relay\"",
    "build:dll": "NODE_ENV=production webpack --config ./webpack/webpack.config.dll.js",
    "build:relay": "npm run update-schema && relay-compiler --src ./src --schema build/schema.graphql",
    "build:server": "NODE_ENV=production webpack --config ./webpack/server.babel.js",
    "build:theme": "node ./webpack/utils/buildThemeJSON.babel.js",
    "db:migrate": "npm run db:migrate-up-all",
    "db:migrate-testing": "NODE_ENV=test npm run db:migrate && NODE_ENV=test node ./src/server/__tests__/setup/softenDurability.babel.js",
    "db:migrate-up": "node ./src/server/database/migrate.babel.js up",
    "db:migrate-up-all": "node ./src/server/database/migrate.babel.js up --all",
    "db:migrate-down": "node ./src/server/database/migrate.babel.js down",
    "db:migrate-down-all": "node ./src/server/database/migrate.babel.js down --all",
    "db:migrate-create": "cd ./src/server/database && migrate-rethinkdb create",
    "debug:stripe": "node ./src/server/billing/debug.babel.js",
    "flow": "flow",
    "test": "jest --bail --coverage --ci --runInBand && codecov",
    "test:dev": "jest --runInBand --watch",
    "test:intranetToken": "node ./src/server/utils/getIntranetToken.babel.js",
    "test:e2e-deps": "node ./e2e/scripts/ensureDrivers.babel.js",
    "test:e2e": "node ./e2e/scripts/runTests.babel.js",
    "lint": "yarn flow && eslint 'src/**/*.js' 'webpack/*.js'",
    "lint:fix": "eslint --fix 'src/**/*.js' 'webpack/*.js' ",
    "profile": "NODE_ENV=development webpack --config ./webpack/dev.babel.js --profile --json > stats.json",
    "relay": "npm run update-schema && relay-compiler --src ./src --schema build/schema.graphql --watch",
    "update-schema": "node ./src/server/utils/updateSchema.babel.js",
    "postversion": "git push --follow-tags"
  },
